{
  "name": "traceurs",
  "version": "0.0.0",
  "description": "Fetch your data, eagerly.",
  "private": true,
  "scripts": {
    "format": "run-p format:md format:json format:source format:yml",
    "format:json": "prettier --parser json --write \"**/*.json\"",
    "format:md": "prettier --parser markdown --write \"./*.md\" \"./**/*.md\"",
    "format:source": "prettier --config ./package.json \"./**/*.{ts,tsx,js}\" --write",
    "format:yml": "prettier --parser yaml --write \"./*.{yml,yaml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brooooooklyn/traceurs.git"
  },
  "keywords": ["React", "SSR", "fetch", "axios"],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brooooooklyn/traceurs/issues"
  },
  "homepage": "https://github.com/Brooooooklyn/traceurs#readme",
  "devDependencies": {
    "ava": "3.15.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@commitlint/config-lerna-scopes": "12.1.4",
    "@swc-node/register": "1.5.2",
    "@types/node": "16.11.60",
    "@types/react-dom": "18.0.5",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "codecov": "3.8.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rxjs": "7.5.6",
    "shx": "0.3.4",
    "sinon": "11.1.2",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.8.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  }
}
